<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>PRODUCTS</title>
  </head>
  <body>
    <h2>Create Tokimon</h2>
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>weight</th>
            <th>height</th>
            <th>fly</th>
            <th>fight</th>
            <th>fire</th>
            <th>water</th>
            <th>electric</th>
            <th>frozen</th>
            <th>total</th>
            <th>trainner name</th>
        </tr>
        <% rows.forEach((r) => { %>
            <tr>
                <td><%= r.id %></td>
                <td><%= r.name %></td>
                <td><%= r.weight %></td>
                <td><%= r.height %></td>
                <td><%= r.fly %></td>
                <td><%= r.fight %></td>
                <td><%= r.fire %></td>
                <td><%= r.water %></td>
                <td><%= r.electric %></td>
                <td><%= r.frozen %></td>
                <td><%= r.total %></td>
                <td><%= r.trainer %></td>
            </tr>
        <% }); %>
    </table>
  </body>
</html>
